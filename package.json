{
  "name": "api-hub-frontend",
  "version": "1.0.0",
  "devDependencies": {
    "jasmine": "^4.5.0",
    "jasmine-console-reporter": "^3.1.0",
    "jsdom": "^20.0.3",
    "jshint": "^2.13.6"
  },
  "scripts": {
    "test": "jasmine --config=jsTest/jasmine.json --reporter=jasmine-console-reporter",
    "jshint": "jshint --verbose app/assets/javascripts/*.js jsTest/tests/*.js"
  },
  "license": "Apache-2.0",
  "type": "module",
  "jshintConfig": {
    "esversion": 8,
    "browser": true,
    "jasmine": true,
    "globals": {"console":  true, "accessibleAutocomplete":  true, "globalThis":  true, "ace":  true},
    "curly": true,
    "eqeqeq": true,
    "futurehostile": true,
    "latedef": true,
    "leanswitch": true,
    "noarg": true,
    "nocomma": true,
    "nonbsp": true,
    "nonew": true,
    "noreturnawait": true,
    "singleGroups": true,
    "strict": "implied",
    "undef": true,
    "unused": true,
    "varstmt": true
  }
}
